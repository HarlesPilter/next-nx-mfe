{
  "name": "mfe-nx-sandbox",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start": "nx run-many --target=serve --output-style=stream",
    "start:storybook-ui": "nx run ui:storybook",
    "build": "nx build && nx run-many --target=build --verbose --output-style=stream",
    "build:storybook-ui": "nx run ui:build-storybook",
    "build:ios-app": "nx build && nx run host:export && nx run host:sync:ios",
    "build:desktop-app": "nx build && nx run host:export && nx run host:sync:desktop",
    "open:ios-app": "nx run host:open:ios",
    "open:desktop-app": "nx run host:open:desktop",
    "commit": "git-cz",
    "lint": "nx lint",
    "format": "nx format:write",
    "stylelint": "nx affected --target=stylelint",
    "stylelint:fix": "nx affected --target=stylelint --fix",
    "test": "nx test"
  },
  "private": true,
  "dependencies": {
    "@capacitor/core": "^3.6.0",
    "@module-federation/nextjs-mf": "^5.11.0",
